	"compilerOptions": {
		"declaration": false,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"module": "commonjs",
		"noImplicitAny": false,
		"noLib": false,
		"removeComments": true,
		"sourceMap": true,
		"target": "es5"
	},
	"exclude": [
		"bower_components",
		"jspm_packages",
		"node_modules"
	]
}