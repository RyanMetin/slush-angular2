{
  "version": "0.0.1",
  "name": "<%= camel %>",
  "description": "Angular2 Using TypeScript",
  "devDependencies": {
    "browser-sync": "^2.9.7",
    "jspm": "^0.16.11",
    "tsd": "^0.6.4"
  },
  "jspm": {
    "directories": {},
    "dependencies": {
      "angular2": "npm:angular2@^2.0.0-alpha.37",
      "reflect-metadata": "npm:reflect-metadata@^0.1.2",
      "rx": "npm:rx@4.0.0",
      "zone.js": "npm:zone.js@^0.5.7"
    },
    "devDependencies": {
      "typescript": "npm:typescript@^1.6.2"
    }
  },
  "main": "src/boot",
  "scripts": {
    "postinstall": "npm run jspm & npm run tsd && npm start",
    "jspm": "jspm install",
    "tsd": "tsd install angular2 es6-promise rx rx-lite systemjs -rs",
    "serve": "browser-sync start --server --open local --files src/*",
    "start": "npm run serve"
  }
}
